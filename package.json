  },
  "scripts": {
    "build:multi": "cross-env webpack --config webpack/webpack.config.js --progress --profile --colors",
    "build": "cross-env NODE_ENV=development webpack --config configs/webpack.config.prod.js --progress --profile --colors",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --progress --profile --colors",
    "build:dt": "cross-env NODE_ENV=dt webpack --config webpack/webpack.config.js --progress --profile --colors",
    "clean": "cross-env rimraf dist coverage node_modules test-reports",
    "coverage": "cross-env ./node_modules/jest/bin/jest.js --coverage --collectCoverageFrom=app/**/*.{js,jsx} --collectCoverageFrom=!app/*.{js,jsx}",
    "lint": "eslint \"app/**\"",
    "lint:fix": "eslint \"app/**\" --fix",
    "start": "node configs/devServer.js --display-entrypoints",
    "test": "cross-env ./node_modules/jest/bin/jest.js",
    "test-dev": "cross-env ./node_modules/jest/bin/jest.js --watch",
    "manage:translations": "node ./translationManager.js",
    "testFile": "cross-env ./node_modules/jest/bin/jest.js --no-cache"
  },
  "jest": {
    "moduleNameMapper": {
      "^app(.*)$": "<rootDir>/app$1"
    },
    "notify": true,
    "setupTestFrameworkScriptFile": "./configs/setup-jasmine-env.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test"
    }
  },
